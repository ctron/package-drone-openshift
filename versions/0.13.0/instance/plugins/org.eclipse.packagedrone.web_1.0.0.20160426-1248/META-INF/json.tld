<?xml version="1.0" encoding="ISO-8859-1" ?>
<taglib xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
	version="2.1">

	<description>JSON Framework JSP Tag Library</description>
	<tlib-version>1.0</tlib-version>
	<short-name>json</short-name>
	<uri>http://eclipse.org/packagedrone/web/json</uri>

	<function>
		<description>Convert a single item, an array of items or a collection of items to a JSON array, filled with strings generated by calling toString on the elements</description>
		<name>array</name>
		<function-class>org.eclipse.packagedrone.web.tags.JsonFunctions</function-class>
		<function-signature>java.lang.String toJson(java.lang.Object)</function-signature>
	</function>
</taglib>